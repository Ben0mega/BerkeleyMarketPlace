<!--<div class="container" id="adminPage">-->
  <div class = "row">
    <div class ="col left s12 m12 l12">
      <h5>Welcome Admin, you are logged in as <span class="user_name"><%= current_user.name %></span> </h5>
    </div>
  </div>
  <div class="divider"></div>
  
  <div class="section"> 
    <h5 class="sub_header">Statistics Blocks</h5>
    <div class="row">
      <div class="col s12 m6 l6">
        <%= render partial: 'users/user_stats' %>
      </div>
      <div class="col s12 m6 l6">
        <%= render partial: 'users/post_stats' %>
      </div>
    </div>
  </div>
  <div class="divider"></div>
  
  <div class="section">  
    <h5 class="sub_header">Pie Charts</h5>
    <div class="row">
      <div class="col s12 m6 l6">
        <%= render partial: 'graphs/post_condition' %>
      </div>
      <div class="col s12 m6 l6">
        <%= render partial: 'graphs/category_condition' %>
      </div>
    </div>
  </div>
  <div class="divider"></div>

  <div class="section">  
    <h5 class="sub_header">Time Series</h5>
    <div class="row">
      <div class="col-1"></div>
      <div class="col s12 m11 l11">
        <%= render partial: 'graphs/post_timeseries' %>
      </div>
      
    </div>
  </div>
  <div class="divider"></div>
  
  
  <div class="section">   
    <h5 class="sub_header">Manage Admin</h5>
    <div class="row">
      <%= render partial: 'users/admin_list' %>
    </div>
  </div>
  
<!--</div>-->